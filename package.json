{
  "type": "module",
  "version": "2.6.3",
  "name": "cobaltwebtech",
  "description": "Cobalt Web Technologies website built on Astro",
  "author": "Cobalt Web Technologies",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@10.17.1",
  "scripts": {
    "dev": "astro dev",
    "dev:cloudflare": "astro build && pnpx wrangler dev",
    "format": "prettier . --write",
    "lint": "eslint .",
    "build": "astro build",
    "preview": "astro preview",
    "upforce": "pnpm up --latest --recursive '*'",
    "up:astro": "pnpx @astrojs/upgrade",
    "astro": "astro",
    "clean": "rimraf dist .astro node_modules pnpm-lock.yaml yarn.lock .yarn package-lock.json .wrangler"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "12.6.9",
    "@astrojs/sitemap": "3.6.0",
    "@playform/compress": "^0.2.0",
    "@tailwindcss/vite": "^4.1.14",
    "astro": "5.14.1",
    "astro-cloudinary": "^1.3.4",
    "astro-compressor": "^1.1.2",
    "astro-font": "^1.1.0",
    "astro-icon": "^1.1.5",
    "gsap": "^3.13.0",
    "lenis": "^1.3.11",
    "preline": "^3.2.3",
    "resend": "^6.1.2",
    "tailwind-variants": "^3.1.1",
    "tailwindcss": "^4.1.14",
    "tw-animate-css": "^1.4.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@types/cloudflare-turnstile": "^0.2.2",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.37.0",
    "eslint-plugin-astro": "^1.3.1",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0",
    "wrangler": "^4.40.3"
  }
}
