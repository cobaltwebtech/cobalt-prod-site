{
  "type": "module",
  "version": "2.5.5",
  "name": "cobaltwebtech",
  "description": "Cobalt Web Technologies website built on Astro",
  "author": "Cobalt Web Technologies",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@10.12.4",
  "scripts": {
    "dev": "astro dev",
    "dev:cloudflare": "astro build && pnpx wrangler dev",
    "format": "prettier . --write",
    "lint": "eslint .",
    "build": "astro build",
    "preview": "astro preview",
    "update": "up --latest --recursive '*'",
    "update:astro": "pnpm dlx @astrojs/upgrade",
    "astro": "astro",
    "clean": "rimraf dist .astro node_modules pnpm-lock.yaml yarn.lock .yarn package-lock.json .wrangler"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "12.6.0",
    "@astrojs/sitemap": "3.4.1",
    "@playform/compress": "^0.2.0",
    "@tailwindcss/vite": "^4.1.11",
    "astro": "5.11.1",
    "astro-cloudinary": "^1.3.2",
    "astro-compressor": "^1.0.0",
    "astro-font": "^1.1.0",
    "astro-icon": "^1.1.5",
    "globby": "^14.1.0",
    "gsap": "^3.13.0",
    "lenis": "^1.3.4",
    "preline": "^3.1.0",
    "resend": "^4.6.0",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.5"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/cloudflare-turnstile": "^0.2.2",
    "@typescript-eslint/parser": "^8.35.1",
    "eslint": "^9.30.1",
    "eslint-plugin-astro": "^1.3.1",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "wrangler": "^4.23.0"
  }
}
