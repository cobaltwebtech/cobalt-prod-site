{
  "type": "module",
  "version": "2.7.3",
  "name": "cobaltwebtech",
  "description": "Cobalt Web Technologies website built on Astro",
  "author": "Cobalt Web Technologies",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@10.19.0",
  "scripts": {
    "dev": "astro dev",
    "dev:cf": "astro build && pnpx wrangler dev",
    "format": "biome format --write",
    "lint": "eslint .",
    "build": "astro build",
    "preview": "astro preview",
    "upforce": "pnpm up --latest --recursive '*'",
    "up:astro": "pnpx @astrojs/upgrade",
    "astro": "astro",
    "clean": "rimraf dist .astro node_modules pnpm-lock.yaml yarn.lock .yarn package-lock.json .wrangler"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.5",
    "@astrojs/cloudflare": "12.6.10",
    "@astrojs/sitemap": "3.6.0",
    "@playform/compress": "^0.2.0",
    "@tailwindcss/vite": "^4.1.16",
    "astro": "5.15.3",
    "astro-cloudinary": "^1.3.5",
    "astro-compressor": "^1.2.0",
    "astro-font": "^1.1.0",
    "astro-icon": "^1.1.5",
    "gsap": "^3.13.0",
    "lenis": "^1.3.13",
    "preline": "^3.2.3",
    "resend": "^6.3.0",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^3.1.1",
    "tailwindcss": "^4.1.16",
    "tw-animate-css": "^1.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@types/cloudflare-turnstile": "^0.2.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.3",
    "wrangler": "^4.45.2"
  }
