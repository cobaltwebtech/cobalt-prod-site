{
  "type": "module",
  "version": "2.7.6",
  "name": "cobaltwebtech",
  "description": "Cobalt Web Technologies website built on Astro",
  "author": "Cobalt Web Technologies",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@10.23.0",
  "scripts": {
    "dev": "astro dev",
    "dev:cf": "astro build && pnpx wrangler dev",
    "format": "biome format --write",
    "lint": "biome lint",
    "build": "astro build",
    "preview": "astro preview",
    "upforce": "pnpm up --latest --recursive '*'",
    "up:astro": "pnpx @astrojs/upgrade",
    "up:biome": "pnpm up @biomejs/biome@latest && pnpm biome migrate --write",
    "astro": "astro",
    "clean": "rimraf .astro .wrangler dist node_modules pnpm-lock.yaml"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/cloudflare": "12.6.11",
    "@astrojs/sitemap": "3.6.0",
    "@playform/compress": "^0.2.0",
    "@tailwindcss/vite": "^4.1.17",
    "astro": "5.16.0",
    "astro-cloudinary": "^1.3.5",
    "astro-compressor": "^1.2.0",
    "astro-font": "^1.1.0",
    "astro-icon": "^1.1.5",
    "gsap": "^3.13.0",
    "lenis": "^1.3.15",
    "preline": "^3.2.3",
    "resend": "^6.5.2",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.1",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@tailwindcss/forms": "^0.5.10",
    "@types/cloudflare-turnstile": "^0.2.2",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3",
    "wrangler": "^4.50.0"
  }
}
