    "astro-vtbot": "^2.0.5",
    "eslint": "^9.19.0",
    "eslint-plugin-astro": "^1.3.1",
    "lint-staged": "^15.4.3",
    "prettier": "^3.4.2",
    "prettier-config-standard": "^7.0.0",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0"
