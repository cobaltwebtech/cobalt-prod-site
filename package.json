{
  "type": "module",
  "version": "2.2.3",
  "name": "cobaltwebtech",
  "description": "Cobalt Web Technologies website built on Astro",
  "author": "Cobalt Web Technologies",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "format": "prettier . --write",
    "lint": "eslint .",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "update": "yarn up -R '*' && yarn up",
    "update:astro": "yarn dlx @astrojs/upgrade",
    "cc": "yarn cache clean",
    "astro": "astro",
    "clean": "rimraf dist .astro node_modules yarn.lock .yarn .pnpm-lock.yaml .vercel"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/sitemap": "3.3.0",
    "@astrojs/vercel": "8.1.3",
    "@playform/compress": "^0.1.9",
    "@sentry/astro": "^9.11.0",
    "@tailwindcss/vite": "^4.1.3",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "astro": "5.6.2",
    "astro-compressor": "^1.0.0",
    "astro-font": "^1.0.0",
    "astro-icon": "^1.1.5",
    "globby": "^14.1.0",
    "gsap": "^3.12.7",
    "lenis": "^1.2.3",
    "preline": "^3.0.1",
    "resend": "^4.2.0",
    "sharp": "^0.34.0",
    "tailwindcss": "^4.1.3"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^9.24.0",
    "eslint-plugin-astro": "^1.3.1",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.0"
  }
}
