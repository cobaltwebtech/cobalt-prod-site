    "@playform/compress": "^0.1.7",
    "@preline/accordion": "^2.6.0",
    "@preline/collapse": "^2.6.0",
    "@preline/dropdown": "^2.6.0",
    "@preline/overlay": "^2.6.0",
    "@preline/tabs": "^2.6.0",
    "@vercel/analytics": "^1.4.1",
    "@vercel/speed-insights": "^1.1.0",
    "astro": "5.2.5",
    "astro-compressor": "^1.0.0",
    "astro-font": "^1.0.0",
    "clipboard": "^2.0.11",
    "globby": "^14.0.2",
    "gsap": "^3.12.7",
    "lenis": "^1.1.20",
